@if (User.Identity.IsAuthenticated)
{
    <ul class="navbar-nav" id="loginOn">
        <li class="nav-item">
            <p class="nav-link nav-up" id="idUser" title="Manage">@User.Identity.Name</p>
        </li>
        <li class="nav-item">
            <a class="nav-link nav-up" title="Manage" asp-controller="Account" asp-action="Logout">Выход</a>
        </li>
    </ul>
}
else
{
    <ul class="navbar-nav" id="loginOf">
        <li class="nav-item">
            <a class="nav-link nav-up" asp-controller="Account" asp-action="Register">Регистрация</a>
        </li>
        <li class="nav-item">
            <a class="nav-link nav-up" asp-controller="Account" asp-action="Login">Вход</a>
        </li>
    </ul>
}
